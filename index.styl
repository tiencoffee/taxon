@import "https://fonts.googleapis.com/css?family=Roboto:400,700"
@font-face
	font-family "TaxonLine"
	src url("https://cdn.jsdelivr.net/gh/tiencoffee/data/TaxonLine4.woff2") format("woff2")
body
	margin 0
	font-family "Roboto",sans-serif
	font-size 13px
	overflow hidden
	user-select none
	cursor default
::-webkit-scrollbar
	-webkit-appearance none
	width 16px
	height 16px
	background #333436
::-webkit-scrollbar-thumb
	background #434446
.line
	height 15px
	font-family "TaxonLine"
.lineFind
	> .node
		border-radius 4px
		background #434446
		box-shadow -4px 1px 0 1px #434446,4px 1px 0 1px #434446
.node
	margin-left 6px
	display inline-block
	font-family "Roboto",sans-serif
	vertical-align middle
.text
	margin-left 8px
	font-size 13px
	color #858164
.img
	min-width 4px
	min-height 4px
	max-width 13px
	max-height 13px
	margin-left 8px
	border-radius 1px
	vertical-align middle
	filter contrast(1.2) saturate(1.2)
	background #fff
	+ .img
		margin-left 2px
.life
	color #d13913
.domain
	color #d13913
.kingdom
	color #db3737
.phylum
	color #db2c6f
.class
	color #d9822b
.order
	color #00b3a4
.family
	color #9bbf30
.tribe
	color #c04dd5
.genus
	color #d4ad5a
.species
	font-weight normal
	color #ced9e0
.subspecies
	font-weight normal
	color #ced9e0
#scrollEl
	position absolute
	left 0
	top 0
	width 100%
	height 100%
	overflow-x hidden
	overflow-y auto
#heightEl
	width 1px
	display inline-block
	pointer-events none
#presEl
	position absolute
	left 0
	top 0
	width 100%
	height 100%
	margin 0
	white-space pre
#infosEl
	position absolute
	right 16px
	bottom 0
	padding 16px
	border-radius 8px 4px 4px 4px
	display grid
	grid-template-columns repeat(2,auto)
	grid-gap 4px 16px
	background #fff
#findEl
	position absolute
	top 0
	right 16px
	width 300px
	padding 6px
	border-radius 4px 4px 4px 8px
	display flex
	align-items center
	background #fff
#findInputEl
	width 0
	height 24px
	padding 0 8px
	border 0
	flex-grow 1
	font inherit
	outline 0
.findButton
	width 28px
	margin-left 4px
	border-radius 4px
	display flex
	justify-content center
	align-items center
	align-self stretch
	font-family monospace
	font-size 15px
	&:hover
		background #07d3
.findButtonOn
	color #07d
#findClose
	color #e43
	&:hover
		background #e433
#findTextEl
	margin 0 8px 0
	font-size 13px
	color #888
#popupEl
	max-width min-content
	border-radius 8px
	text-align center
	overflow hidden
	box-shadow 0 1px 8px 2px #3336
#popupBody
	padding 8px
	display flex
	flex-direction column
	background #fff
	animation popup 50ms forwards
	pointer-events none
@keyframes popup
	from
		transform scale(.96)
	to
		transform scale(1)
.popupTwoImg
	.popupImg
		max-width 280px
		max-height 240px
#popupName
	display block
	font-weight bold
	white-space nowrap
#popupText
	margin-bottom -2px
	white-space nowrap
	color #333
#popupGenders
	margin-top 6px
	display flex
	justify-content center
.popupGender
	width 100%
	display flex
	flex-direction column
	align-self stretch
	&:nth-child(2)
		margin-left 4px
.popupPicture
	position relative
	border-radius 8px
	display flex
	justify-content center
	align-items center
	flex-grow 1
	overflow hidden
	background #fff
.popupBgImg
	position absolute
	left 0
	top 0
	width 100%
	height 100%
	object-fit cover
	filter blur(24px)
.popupImg
	max-width 320px
	max-height 240px
	filter contrast(1.1) saturate(1.1)
	image-rendering -webkit-optimize-contrast
	z-index 0
.popupCaptn
	margin-top 2px
	font-size 13px
	color #333
.popupGenderCaptn
	margin-top 2px
	font-size 13px
	color #333
#popupSummary
	margin-top 6px
	font-size 13px
	line-height 1.3
	> p
		margin 0
		text-align justify
