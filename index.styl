body
	margin 0
	font-family "Roboto",sans-serif
	font-size 10pt
::-webkit-scrollbar
	-webkit-appearance none
	background #333436
::-webkit-scrollbar-thumb
	background #434446
.line
	height 15px
	font-family "DejaVu Sans Mono"
.lineFind
	background #434446
.node
	margin-left 6px
	display inline-block
	font-family "Roboto",sans-serif
	vertical-align middle
.text
	margin-left 8px
	color #858164
.img
	min-width 4px
	min-height 4px
	max-width 13px
	max-height 13px
	margin-left 8px
	border-radius 2px
	vertical-align middle
	background #fff
.life
	color #d13913
.domain
	color #d13913
.kingdom
	color #db3737
.phylum
	color #db2c6f
.class
	color #d9822b
.order
	color #00b3a4
.family
	color #9bbf30
.tribe
	color #c04dd5
.genus
	color #d4ad5a
.species
	font-weight normal
	color #ced9e0
.subspecies
	font-weight normal
	color #ced9e0
#presEl
	position fixed
	left 0
	top 0
	width 100%
	height 100%
	margin 0
	white-space pre
	cursor default
#findEl
	position fixed
	top 0
	right 0
	width 260px
	padding 8px 12px
	border-radius 3px
	display flex
	background #fff
#findInputEl
	margin-right 16px
	border 0
	flex-grow 1
	outline 0
#findTextEl
	color #888
#popupBody
	max-width min-content
	padding 8px
	border-radius 6px
	display flex
	flex-direction column
	font-family "Roboto",sans-serif
	text-align center
	background #fff
	transform-origin 100% 50%
	pointer-events none
.popupTwoImg
	.popupImg
		max-width 280px
		max-height 240px
#popupName
	display block
	font-weight bold
	white-space nowrap
#popupText
	margin-bottom -2px
	white-space nowrap
	color #333
#popupGenders
	margin-top 6px
	display flex
	justify-content center
.popupGender
	width 100%
	display flex
	flex-direction column
	align-self stretch
	&:nth-child(2)
		margin-left 4px
.popupPicture
	position relative
	border-radius 6px
	display flex
	justify-content center
	align-items center
	flex-grow 1
	overflow hidden
	background #fff
.popupBgImg
	position absolute
	left 0
	top 0
	width 100%
	height 100%
	object-fit cover
	filter blur(24px)
.popupImg
	max-width 320px
	max-height 240px
	image-rendering -webkit-optimize-contrast
	z-index 0
.popupCaptn
	margin-top 2px
	color #333
.popupGenderCaptn
	margin-top 2px
	color #333
#popupSummary
	margin-top 6px
	line-height 1.3
	> p
		margin 0
		text-align justify
