{
	"name": "taxon",
	"version": "0.1",
	"manifest_version": 2,
	"author": "tiencoffee",
	"description": "taxon",
	"content_scripts": [{
		"matches": [
			"*://*.planetcatfish.com/*",
			"*://*.fishbase.se/*",
			"*://*.fishbase.in/*",
			"*://*.bugguide.net/*",
			"*://*.biolib.cz/*",
			"*://*.wikipedia.org/*",
			"*://*.wikimedia.org/*",
			"*://imgur.com/*",
			"*://www.google.com/*",
			"*://www.google.com.vn/*"
		],
		"js": [
			"libs/mithril.min.js",
			"libs/cropper.min.js",
			"misc/extension.js"
		],
		"css": [
			"libs/cropper.min.css",
			"misc/extension.css"
		],
		"run_at": "document_start",
		"all_frames": false
	}],
	"background": {
		"scripts": [
			"misc/background.js"
		],
		"persistent": true
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"permissions": [
		"tabs",
		"storage",
		"http://localhost:8080/*"
	]
}
