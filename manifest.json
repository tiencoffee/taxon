{
	"name": "taxon",
	"version": "0.1",
	"manifest_version": 2,
	"author": "tiencoffee",
	"description": "Taxonomy",
	"content_scripts": [{
		"matches": [
			"*://*.wikipedia.org/*",
			"*://*.wikimedia.org/*",
			"*://imgur.com/*",
			"*://www.google.com/*",
			"*://www.google.com.vn/*"
		],
		"js": [
			"libs/mithril.min.js",
			"libs/cropper.min.js",
			"extension.js"
		],
		"css": [
			"libs/cropper.min.css",
			"extension.css"
		],
		"run_at": "document_start",
		"all_frames": false
	}],
	"background": {
		"scripts": [
			"background.js"
		],
		"persistent": false
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"permissions": [
		"tabs",
		"storage"
	]
}
